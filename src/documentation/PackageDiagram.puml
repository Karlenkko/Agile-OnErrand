@startuml

'abstract class AbstractList
'abstract AbstractCollection
'interface List
'interface Collection
'
'List <|-- AbstractList
'Collection <|-- AbstractCollection
'
'Collection <|- List
'AbstractCollection <|- AbstractList
'AbstractList <|-- ArrayList
'
'class ArrayList {
'Object[] elementData
'size()
'}
'
'enum TimeUnit {
'DAYS
'HOURS
'MINUTES
'}

Package Controller
Package Data
Package Algorithm
Package Model
Package Test
Package View
Package Observer

View <-down-> Controller
Controller -up-> Model
View -> Model
Algorithm <- Controller
Controller -> Data

Data -down-> Test
Algorithm -down-> Test
View -> Observer

@enduml